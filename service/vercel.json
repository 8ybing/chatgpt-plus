{
  "version": 2,
  "name": "ChatGPT-Plus-service",
  "buildCommand": "npm build",
  "installCommand": "npm install",
  "outputDirectory": ".",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
